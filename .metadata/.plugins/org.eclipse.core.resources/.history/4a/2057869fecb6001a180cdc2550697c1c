package com.microservice.coursecatalogueservice.resource;

import java.util.Collections;
import java.util.List;

import org.springframework.web.bind.annotation.RestController;

import com.microservice.coursecatalogueservice.model.Catalogue;

@RestController
public class CourceCatalogueResource {
	
	public List<Catalogue> getCatalogue(String userName){
		return Collections.singletonList(new Catalogue("Introduction to java", description, technology, keywords, rating));
	}

}
